{
	"name": "网易邮箱大师清理",
	"author": "延时qwq",
	"version_name": "v1.0",
	"version_code": 1,
	"platform": "windows",
	"requires": [
		"${PROGRAMFILES(X86)}\\Netease\\MailMaster\\Application\\mailmaster.exe"
	],
	"rules": [
		{
			"name": "网易邮箱大师dump文件",
			"warning_level": 0,
			"paths": [
				"${LOCALAPPDATA}\\Netease\\MailMaster\\dumps\\.+"
			]
		},{
			"name": "网易邮箱大师日志文件",
			"warning_level": 0,
			"paths": [
				"${LOCALAPPDATA}\\Netease\\MailMaster\\logs\\.+"
			]
		},{
			"name": "网易邮箱大师缓存文件",
			"warning_level": 0,
			"paths": [
				"${LOCALAPPDATA}\\Netease\\MailMaster\\web\\cache\\.+"
			]
		}
	]
}