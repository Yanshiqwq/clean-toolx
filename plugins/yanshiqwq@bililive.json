{
	"name": "B站投稿工具清理",
	"author": "延时qwq",
	"version_name": "v1.0",
	"version_code": 1,
	"platform": "windows",
	"rules": [
		{
			"name": "B站投稿工具Url缓存",
			"warning_level": 0,
			"folders": [
				{
					"path": "${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\url_cache\\",
					"extension": "*"
				}
			]
		},{
			"name": "B站投稿工具日志",
			"warning_level": 0,
			"files": [
				"${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\bililive_browser_debug.log",
				"${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\bililive_debug.log"
			]
		}
	]
}