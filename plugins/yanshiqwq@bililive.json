{
	"name": "B站投稿工具清理",
	"author": "延时qwq",
	"version_name": "v1.1",
	"version_code": 2,
	"platform": "windows",
	"requires": [
		"${PROGRAMFILES(X86)}\\bililive\\ugc_assistant\\ugc_assistant.exe"
	],
	"rules": [
		{
			"name": "B站投稿工具缓存",
			"warning_level": 0,
			"paths": [
				{
					"scan_path": "${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\url_cache\\",
					"path": "${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\url_cache\\.+"
				},{
					"scan_path": "${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\",
					"path": "${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\\\d+\\avatar.jpg"
				},{
					"scan_path": "${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\",
					"path": "${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\Browser Cache \\d+\\Vistied links"
				}
			]
		},{
			"name": "B站投稿工具日志",
			"warning_level": 0,
			"paths": [
				{
					"scan_path": "${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\",
					"path": "${USERPROFILE}\\AppData\\Local\\bililive\\User Data\\bililive_.*debug.log"
				}
			]
		}
	]
}