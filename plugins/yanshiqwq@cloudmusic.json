{
	"name": "网易云清理",
	"author": "延时qwq",
	"version_name": "v1.0",
	"version_code": 1,
	"min_version": 1,
	"platform": "windows",
	"rules": [
		{
			"name": "网易云广告缓存",
			"warning_level": 0,
			"folders": [
				{
					"path": "${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\ad\\",
					"extension": ".mdat"
				}
			]
		},{
			"name": "网易云在线歌曲缓存",
			"warning_level": 0,
			"folders": [
				{
					"path": "${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\Cache\\Cache\\",
					"extension": "*"
				}
			]
		},{
			"name": "网易云DMP文件",
			"warning_level": 0,
			"folders": [
				{
					"path": "${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\dumps\\",
					"extension": ".dmp"
				}
			]
		},{
			"name": "网易云日志",
			"warning_level": 1,
			"folders": [
				{
					"path": "${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\Log\\",
					"extension": "*"
				}
			],
			"files": [
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\cloudmusic.log",
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\install.log",
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\web.log"
			]
		},{
			"name": "网易云用户数据及设置",
			"warning_level": 1,
			"folders": [
				{
					"path": "${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webapp\\",
					"extension": "*"
				}
			],
			"excludes": [
				{
					"path": "${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webapp\\Cookies",
					"strict": true
				},{
					"path": "${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webapp\\Cookies-journal",
					"strict": true
				},{
					"path": "data_",
					"strict": false
				},{
					"path": "${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webapp\\Local Storage\\orpheus_orpheus_0.localstorage",
					"strict": true
				},{
					"path": "${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webapp\\Local Storage\\orpheus_orpheus_0.localstorage-journal",
					"strict": true
				}
			]
		}
	]
}