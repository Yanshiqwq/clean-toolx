{
	"name": "网易云清理",
	"author": "延时qwq",
	"version_name": "v1.1",
	"version_code": 2,
	"platform": "windows",
	"requires": [
		"${PROGRAMFILES(X86)}\\Netease\\CloudMusic\\CloudMusic.exe"
	],
	"rules": [
		{
			"name": "网易云广告缓存",
			"warning_level": 0,
			"paths": [
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\ad\\.+\\\.mdat"
			]
		},{
			"name": "网易云在线歌曲缓存",
			"warning_level": 0,
			"paths": [
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\Cache\\Cache\\.+"
			]
		},{
			"name": "网易云DMP文件",
			"warning_level": 0,
			"paths": [
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\dumps\\.+\\\.dmp"
			]
		},{
			"name": "网易云日志",
			"warning_level": 0,
			"paths": [
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\Log\\.+",
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\.+.log$"
			]
		},{
			"name": "网易云内置浏览器缓存",
			"warning_level": 0,
			"paths": [
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webapp\\(GPUCache\\)?data_[0-9]+",
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webapp\\f_[0-9|a-f]{6,6}",
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webapp\\(GPUCache\\)?index",
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webdata\\lyric\\[0-9]+",
				"${USERPROFILE}\\AppData\\Local\\Netease\\CloudMusic\\webdata\\file\\.+"
			]
		}
	]
}