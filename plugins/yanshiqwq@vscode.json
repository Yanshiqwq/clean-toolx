{
	"name": "VSCode清理",
	"author": "延时qwq",
	"version_name": "v1.0",
	"version_code": 1,
	"min_version": 1,
	"platform": "windows",
	"requires": [
		"${USERPROFILE}\\AppData\\Local\\Programs\\Microsoft VS Code\\Code.exe"
	],
	"rules": [
		{
			"name": "VSCode缓存",
			"warning_level": 0,
			"paths": [
				"${APPDATA}\\Code\\Cache\\.+",
				"${APPDATA}\\Code\\CachedData\\.+",
				"${APPDATA}\\Code\\CachedExtensions\\.+",
				"${APPDATA}\\Code\\CachedExtensionsVSIXs\\.+",
				"${APPDATA}\\Code\\Code Cache\\.+",
				"${APPDATA}\\Code\\GPUCache\\.+"
			]
		},{
			"name": "VSCode日志",
			"warning_level": 1,
			"paths": [
				"${APPDATA}\\Code\\logs\\.+"
			]
		}
	]
}