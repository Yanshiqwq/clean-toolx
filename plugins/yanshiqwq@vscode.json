{
	"name": "VSCode清理",
	"author": "延时qwq",
	"version_name": "v1.0",
	"version_code": 1,
	"min_version": 1,
	"platform": "windows",
	"requires": [
		"${USERPROFILE}\\AppData\\Local\\Programs\\Microsoft VS Code\\Code.exe"
	],
	"rules": [
		{
			"name": "VSCode缓存",
			"warning_level": 0,
			"paths": [
				{
					"scan_path": "${APPDATA}\\Code\\Cache\\",
					"path": "${APPDATA}\\Code\\Cache\\.+"
				},{
					"scan_path": "${APPDATA}\\Code\\CachedData\\",
					"path": "${APPDATA}\\Code\\CachedData\\.+"
				},{
					"scan_path": "${APPDATA}\\Code\\CachedExtensions\\",
					"path": "${APPDATA}\\Code\\CachedExtensions\\.+"
				},{
					"scan_path": "${APPDATA}\\Code\\CachedExtensionsVSIXs\\",
					"path": "${APPDATA}\\Code\\CachedExtensionsVSIXs\\.+"
				},{
					"scan_path": "${APPDATA}\\Code\\Code Cache\\",
					"path": "${APPDATA}\\Code\\Code Cache\\.+"
				},{
					"scan_path": "${APPDATA}\\Code\\GPUCache\\",
					"path": "${APPDATA}\\Code\\GPUCache\\.+"
				}
			]
		},{
			"name": "VSCode日志",
			"warning_level": 1,
			"paths": [
				{
					"scan_path": "${APPDATA}\\Code\\logs\\",
					"path": "${APPDATA}\\Code\\logs\\.+"
				}
			]
		}
	]
}