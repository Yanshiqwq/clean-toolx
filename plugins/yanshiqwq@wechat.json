{
	"name": "微信清理",
	"author": "延时qwq",
	"version_name": "v1.0",
	"version_code": 1,
	"min_version": 1,
	"platform": "windows",
	"requires": [
		"${PROGRAMFILES(X86)}\\Tencent\\WeChat\\WeChat.exe"
	],
	"rules": [
		{
			"name": "微信内置浏览器缓存",
			"warning_level": 0,
			"paths": [
				"${USERPROFILE}\\AppData\\Local\\xwalk\\(GPUCache\\)?(data_[0-9]|index)+",
				"${USERPROFILE}\\AppData\\Local\\xwalk\\f_[0-9|a-f]{6,6}"
			]
		},{
			"name": "微信缓存",
			"warning_level": 1,
			"paths": [
				"${USERPROFILE}\\Documents\\WeChat Files\\[0-9|a-f|A-F]{32,32}(_t|_t[0-9]|.[0-9])?(\.dat)?",
				"${USERPROFILE}\\Documents\\WeChat Files\\[0-9|a-f]{7,8}_([0-9]+_)?thumb\.dat",
				"${USERPROFILE}\\Documents\\WeChat Files\\(_APP_\.wxapkg|~\$cache[0-9]|log[0-9])"
			]
		}
	]
}