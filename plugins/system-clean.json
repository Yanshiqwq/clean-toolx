{
	"name": "系统清理",
	"author": "延时qwq",
	"version_name": "v1.0",
	"version_code": 1,
	"min_version": 1,
	"platform": "windows",
	"rules": [
		{
			"name": "Windows目录临时文件",
			"warning_level": 1,
			"folders": [
				{
					"path": "C:\\Windows\\Temp\\",
					"extension": "*"
				}
			]
		},{
			"name": "AppData目录临时文件",
			"warning_level": 1,
			"folders": [
				{
					"path": "C:\\Users\\${USERNAME}\\AppData\\Local\\Temp\\",
					"extension": "*"
				},{
					"path": "C:\\Users\\${USERNAME}\\AppData\\LocalLow\\Temp\\",
					"extension": "*"
				}
			]
		}
	]
}